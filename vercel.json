{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.ts"
        }
    ],
    
    "functions": ".",
    "rewrites": [
        {
        "source": "(.*)",
        "function": "$1"
        },
        {
        "source": "(.*)",
        "destination": "$1"
        }
    ],
    "builds": [
        {
        "src": "prisma/schema.prisma",
        "use": "@prisma/cli"
        }
    ]
    }
    